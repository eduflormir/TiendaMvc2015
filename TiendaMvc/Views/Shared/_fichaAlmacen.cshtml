@model TiendaMvc.Models.Almacen

<div class="col-md-3">
    <h3>@Html.DisplayFor(o => Model.nombre) </h3>
    <p>
        @Html.DisplayFor(o => Model.ciudad)
        @Html.DisplayFor(o => Model.codigo_postal)
    </p>
    <p>
        @Html.ActionLink("Modificar", "Modificar", "Almacen",
        new { id = Model.id_almacen },null)

        <a href="#" class="borrame" data-id="@Model.id_almacen">
            Borrar
        </a>
    </p>

</div>
    
